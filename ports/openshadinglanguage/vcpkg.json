{
    "name": "openshadinglanguage",
    "version": "1.14-beta1",
    "port-version": 1,
    "description": "OpenShadingLanguage is an advanced shading language for production rendering.",
    "homepage": "https://github.com/AcademySoftwareFoundation/OpenShadingLanguage",
    "license": "BSD-3-Clause",
    "dependencies": [
        {
            "name": "vcpkg-cmake"
        },
        {
            "name": "vcpkg-cmake-config"
        },
        {
            "name": "openimageio"
        },
        {
            "name": "imath"
        },
        {
            "name": "pugixml"
        },
        {
            "name": "llvm",
            "default-features": false,
            "features": [
                "clang",
                "target-nvptx",
                "target-x86",
                "target-spirv"
            ]
        }
    ]
}